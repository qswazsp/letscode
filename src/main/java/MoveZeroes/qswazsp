public class MoveZeroes {
    //Input: [0,1,0,3,12] Output: [1,3,12,0,0]
    //要求使用稳定排序和原地排序
    public void moveZeroes(int[] nums){
        int index = nums.length - 1;
        for (int i = nums.length - 1; i >= 0; i--) {
            if (nums[i] == 0) {
                int temp = nums[i];
                //移动操作
                for (int j = i; j < index; j++) {
                    nums[j] = nums[j + 1];
                }
                nums[index--] = temp;
            }
        }
    }
}
